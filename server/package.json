{
  "name": "express-babel",
  "version": "1.0.0",
  "description": "Using latest JavaScript features on the server",
  "repository": "https://github.com/vmasto/express-babel",
  "author": "Vassilis Mastorostergios <vmasto@gmail.com>",
  "license": "MIT",
  "main": "src/index.js",
  "engines": {
    "node": "~9.7.1",
    "npm": ">=6.4.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config\"",
    "dev:production": "DATABASE_URL=postgres://ozziegooen@127.0.0.1:5432/predictions NODE_ENV=production npm run dev",
    "thing": "nodemon src/bots/primary.js --exec \"node -r dotenv/config\"",
    "resolutions": "nodemon src/bots/resolution.js --exec \"node -r dotenv/config\"",
    "inspect": "nodemon --inspect src/index.js",
    "test": "jest --watch",
    "test:cv": "jest --coverage --forceExit",
    "test:ci": "jest --forceExit",
    "lint": "esw -w src test",
    "serve": "node ./src/index.js",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:seed": "sequelize db:seed:all",
    "db:reset": "npm run db:drop; npm run db:create; npm run db:migrate"
  },
  "keywords": [
    "express",
    "boilerplate",
    "scaffold",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "jest",
    "eslint"
  ],
  "dependencies": {
    "@slack/client": "^4.8.0",
    "apollo-server": "^2.4.8",
    "apollo-server-express": "^2.1.0",
    "auth0-js": "^9.7.3",
    "body-parser": "^1.18.2",
    "body-parser-graphql": "^1.1.0",
    "cors": "^2.8.4",
    "dataloader-sequelize": "^1.7.2",
    "enforce": "^0.1.7",
    "express": "^4.16.2",
    "express-graphql": "^0.6.12",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^1.0.0",
    "express-sslify": "^1.2.0",
    "graphql": "^14.0.0",
    "graphql-middleware": "^3.0.2",
    "graphql-relay": "^0.5.5",
    "graphql-sequelize": "^9.0.1",
    "graphql-shield": "^5.3.1",
    "graphql-tools": "^3.1.1",
    "graphql-type-json": "^0.2.1",
    "jsonwebtoken": "^8.3.0",
    "jwks-rsa": "^1.3.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "node-fetch": "^2.3.0",
    "node-schedule": "^1.3.2",
    "pg": "^7.4.3",
    "ramda": "^0.25.0",
    "react-icons-kit": "^1.2.1",
    "reason-apollo": "^0.13.0",
    "rimraf": "^2.6.2",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.1.1"
  },
  "devDependencies": {
    "bs-platform": "^3.1.5",
    "dotenv": "^4.0.0",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-watch": "^3.1.3",
    "graphql_ppx": "^0.2.7",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0"
  },
  "eslintConfig": {
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "restoreMocks": true,
    "clearMocks": true,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/manuals/"
    ],
    "setupFiles": [
      "./src/jest-globals.js"
    ]
  }
}
