{
  "name": "@foretold/client",
  "version": "0.2.0",
  "author": "Ozzie Gooen <ozzieagooen@gmail.com>",
  "keywords": [
    "BuckleScript"
  ],
  "engines": {
    "node": "12.6.0",
    "yarn": ">=1.16.0"
  },
  "private": false,
  "license": "MIT",
  "scripts": {
    "build-bsb": "bsb -make-world",
    "clean": "bsb -clean-world",
    "watch": "bsb -make-world -w",
    "bsb-watch": "bsb -clean-world -make-world -w",
    "reset": "yarn run clean && yarn run build-bsb && yarn run bsb-watch",
    "parcel": "API_URL=${API_URL:-http://localhost:4000/graphql} AUTH0_DOMAIN=${AUTH0_DOMAIN:-foretold.auth0.com} AUTH0_CLIENT_ID=${AUTH0_CLIENT_ID:-TBKYEQPqhp3Q0q9JcdHlzlG6WlJPBc3A} INTERCOM_APP_ID=${INTERCOM_APP_ID:-gx67sx4f} parcel ./src/index.html --public-url / --no-autoinstall -- watch",
    "parcel-kill": "pkill -f parcel",
    "parcel-reset": "yarn run parcel-kill && yarn run parcel",
    "parcel-staging": "API_URL=${API_URL:-http://localhost:4000/graphql} AUTH0_DOMAIN=${AUTH0_DOMAIN:-foretold.auth0.com} AUTH0_CLIENT_ID=${AUTH0_CLIENT_ID:-TBKYEQPqhp3Q0q9JcdHlzlG6WlJPBc3A} INTERCOM_APP_ID=${INTERCOM_APP_ID:-gx67sx4f} parcel build ./src/index.html --no-source-maps --no-autoinstall",
    "parcel-production": "API_URL=${API_URL:-http://localhost:4000/graphql} AUTH0_DOMAIN=${AUTH0_DOMAIN:-foretold.auth0.com} AUTH0_CLIENT_ID=${AUTH0_CLIENT_ID:-TBKYEQPqhp3Q0q9JcdHlzlG6WlJPBc3A} INTERCOM_APP_ID=${INTERCOM_APP_ID:-gx67sx4f} parcel build ./src/index.html --no-source-maps --no-autoinstall",
    "prepare-dist": "mkdir -p dist && cp _redirects dist/_redirects",
    "replace-dist": "rm -rf .cache && rm -rf dist && yarn prepare-dist",
    "build": "yarn parcel-production",
    "build-for-server": "API_URL=https://api.foretold.io/graphql AUTH0_DOMAIN=foretold.auth0.com AUTH0_CLIENT_ID=oIh53ohBnA4xqhaO8vZXa7PiW6TCXjMV INTERCOM_APP_ID=gx67sx4f NODE_ENV=development yarn parcel-production",
    "build-for-staging": "API_URL=https://prediction-backend-staging.herokuapp.com/graphql AUTH0_DOMAIN=${AUTH0_DOMAIN:-foretold.auth0.com} AUTH0_CLIENT_ID=${AUTH0_CLIENT_ID:-TBKYEQPqhp3Q0q9JcdHlzlG6WlJPBc3A} INTERCOM_APP_ID=${INTERCOM_APP_ID:-gx67sx4f} yarn parcel-staging",
    "build-for-development": "API_URL=${API_URL:-http://localhost:4000/graphql} AUTH0_DOMAIN=${AUTH0_DOMAIN:-foretold.auth0.com} AUTH0_CLIENT_ID=${AUTH0_CLIENT_ID:-TBKYEQPqhp3Q0q9JcdHlzlG6WlJPBc3A} INTERCOM_APP_ID=${INTERCOM_APP_ID:-gx67sx4f} yarn parcel",
    "build-and-deploy": "yarn clean && yarn build-bsb && yarn build-for-server && yarn server-deploy",
    "serve": "static-server ./dist -p ${PORT} -d -n ./dist/index.html",
    "serve-ws": "ws -d dist --spa index.html",
    "build-t": "yarn build-bsb && yarn build",
    "test": "jest --ci --bail --no-cache",
    "update-gql": "API_URL=${API_URL:-http://localhost:4000/graphql} yarn send-introspection-query ${API_URL}",
    "find-interface-for-file": "bsc -bs-re-out",
    "analyze": "source-map-explorer dist/Index.adc0ecca.js"
  },
  "devDependencies": {
    "@foretold/cdf": "1.0.16",
    "@foretold/guesstimator": "1.0.11",
    "d3": "5.15.0",
    "emotion": "10.0.27",
    "moment": "2.24.0",
    "rc-dropdown": "2.4.1",
    "rc-menu": "7.4.20",
    "vega-embed": "6.2.2",
    "vega": "5.9.1",
    "react-use": "13.24.0",
    "react-textarea-autosize": "7.1.2",
    "autoprefixer": "9.7.4",
    "gh-pages": "2.2.0",
    "jest": "25.1.0",
    "jstat": "1.9.2",
    "mathjs": "5.10.3",
    "moduleserve": "0.9.1",
    "pdfast": "0.2.0",
    "discrete-sampling": "1.0.3",
    "financejs": "4.1.0"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@baransu/graphql_ppx_re": "0.6.1",
    "@foretold/bs-distplus": "0.0.3",
    "@foretold/components": "0.0.5",
    "@glennsl/bs-jest": "0.4.9",
    "@glennsl/bs-json": "5.0.2",
    "antd": "3.17.0",
    "apollo-client": "2.6.4",
    "auth0-js": "9.11.3",
    "bs-ant-design-alt": "2.0.0-alpha.33",
    "bs-css": "11.0.0",
    "bs-ken": "0.0.10",
    "bs-moment": "0.4.5",
    "bs-platform": "7.2.2",
    "bs-reform": "9.7.1",
    "bsb-js": "1.1.7",
    "graphql": "14.6.0",
    "graphql-tag": "2.10.3",
    "graphql_ppx": "0.2.8",
    "jwt-decode": "2.2.0",
    "lenses-ppx": "5.1.0",
    "less": "3.11.1",
    "lib": "4.0.5",
    "lodash": "4.17.15",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-bundle-visualiser": "1.2.0",
    "parcel-plugin-html-externals": "0.1.2",
    "parcel-plugin-less-js-enabled": "1.0.2",
    "rationale": "0.2.0",
    "re-debouncer": "2.1.0",
    "react": "16.12.0",
    "react-apollo": "2.5.8",
    "react-apollo-hooks": "0.5.0",
    "react-copy-to-clipboard": "5.0.2",
    "react-dom": "16.12.0",
    "react-icons-kit": "1.3.1",
    "react-markdown": "4.1.0",
    "reason-apollo": "0.19.0",
    "reason-apollo-hooks": "2.4.0",
    "reason-react": "0.7.0",
    "reschema": "1.3.0",
    "source-map-explorer": "^2.4.2",
    "static-server": "2.2.1",
    "subscriptions-transport-ws": "0.9.16",
    "toml": "2.2.3",
    "warning": "4.0.3"
  },
  "alias": {
    "react": "./node_modules/react",
    "react-dom": "./node_modules/react-dom",
    "bs-platform": "./node_modules/bs-platform",
    "remark-parse": "./node_modules/remark-parse"
  },
  "externals": {
    "/env.js": false
  }
}
